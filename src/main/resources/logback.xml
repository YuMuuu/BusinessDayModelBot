<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE logback>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>
  <!-- <root level="ERROR"> -->
  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>